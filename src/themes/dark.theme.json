{
  "palette": {
    "common": {
      "white": "#ffffff",
      "black": "#000000"
    },
    "primary": {
      "main": "#007acc"
    },
    "grey": {
      "1": "#cccccc",
      "2": "#bbbbbb",
      "3": "#6e6565",
      "4": "#37373d",
      "5": "#4d4d4d",
      "6": "#696868"
    },
    "background": {
      "paper": "#1e1e1e",
      "default": "#1e1e1e"
    }
  },
  "typography": {
    "button": {
      "fontSize": "12px"
    },
    "body1": {
      "fontSize": "12px"
    },
    "body2": {
      "fontSize": "12px"
    },
    "caption": {
      "fontSize": "12px"
    }
  },
  "mixins": {
    "toolbar": {
      "background": "#404040",
      "minHeight": "40px",
      "@media (min-width:0px) and (orientation: landscape)": {
        "minHeight": "40px"
      },
      "@media (min-width:600px)": {
        "minHeight": "40px"
      }
    },
    "footer": {
      "height": "30px",
      "background": "#007acc"
    },
    "activitybar": {
      "width": "56px",
      "background": "#333333"
    },
    "icon": {
      "background": {
        "default": "rgba(255,255,255,0.4)",
        "selected": "#ffffff"
      }
    },
    "sidebar": {
      "background": "#252526"
    },
    "accordion": {
      "summary": {
        "minHeight": "30px",
        "background": "#252526"
      }
    },
    "contextMenu": {
      "background": "#252526",
      "fontColor": {
        "primary": "#B7B7B7",
        "secondary": "#A5A5A5"
      },
      "hover": "#0F4771",
      "divider": "#4B4B4C"
    },
    "panel": {
      "tab": {
        "fontColor": "#858585",
        "selected": "#B1B1B1",
        "active": "#333333"
      }
    },
    "textField": {
      "background": "#3C3C3C",
      "color": "#A6A692"
    }
  }
}
