{
  "palette": {
    "common": {
      "white": "#ffffff",
      "black": "#000000"
    },
    "primary": {
      "main": "#007acc"
    },
    "grey": {
      "1": "#cccccc",
      "2": "#6f6f6f",
      "3": "#6e6565",
      "4": "#e1e1e1",
      "5": "#4d4d4d",
      "6": "rgba(51,51,51,0.7)"
    },
    "background": {
      "paper": "#ffffff",
      "default": "#ffffff"
    },
    "divider": "rgba(97,97,97,0.19)"
  },
  "typography": {
    "button": {
      "fontSize": "12px"
    },
    "body1": {
      "fontSize": "12px"
    },
    "body2": {
      "fontSize": "12px"
    },
    "caption": {
      "fontSize": "12px"
    }
  },
  "mixins": {
    "toolbar": {
      "background": "#404040",
      "minHeight": "40px",
      "@media (min-width:0px) and (orientation: landscape)": {
        "minHeight": "40px"
      },
      "@media (min-width:600px)": {
        "minHeight": "40px"
      }
    },
    "footer": {
      "height": "30px",
      "background": "#007acc"
    },
    "activitybar": {
      "width": "48px",
      "background": "#2c2c2c"
    },
    "icon": {
      "background": {
        "default": "rgba(255,255,255,0.4)",
        "selected": "#ffffff"
      }
    },
    "sidebar": {
      "background": "#f3f3f3"
    },
    "accordion": {
      "color": "#616161",
      "summary": {
        "minHeight": "30px",
        "background": "#f3f3f3",
        "iconBg": "#c4c4c4"
      }
    },
    "contextMenu": {
      "background": "#ffffff",
      "fontColor": {
        "primary": "#616161",
        "secondary": "#A5A5A5",
        "primaryHover": "#ffffff",
        "secondaryHover": "#ffffff"
      },
      "hover": "#006ab1"
    },
    "panel": {
      "tab": {
        "fontColor": "#616161",
        "selected": "#424242",
        "active": "#424242"
      },
      "iconColor": "#424242"
    },
    "textField": {
      "background": "#ffffff",
      "color": "#424242"
    },
    "editorForeHead": {
      "color": "rgba(51,51,51,0.7)",
      "active": "rgba(51,51,51,0.7)",
      "background": "#ffffff",
      "iconColor": "rgba(51,51,51,0.7)",
      "iconBg": "rgba(184,184,184,0.31)",
      "height": "35px"
    },
    "breadcrumb": {
      "color": "rgba(97,97,97,0.8)",
      "iconColor": "#424242"
    },
    "extensionsList": {
      "primary": "#616161",
      "secondary": "#616161"
    },
    "notification": {
      "background": "#e7e7e7",
      "color": "#616161",
      "hoverBg": "rgba(184,184,184,0.31)"
    }
  }
}
